{
  "name": "bootstrap-portfolio-project",
  "version": "1.0.0",
  "description": "I intend to showcase what I've learned in the NuCamp Bootstrap course with a hiking website.",
  "main": "index.html",
  "scripts": {
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreenGimli/bootstrap-portfolio-project.git"
  },
  "author": "Charlie Wallace",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GreenGimli/bootstrap-portfolio-project/issues"
  },
  "homepage": "https://github.com/GreenGimli/bootstrap-portfolio-project#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "node-sass": "^6.0.1",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
